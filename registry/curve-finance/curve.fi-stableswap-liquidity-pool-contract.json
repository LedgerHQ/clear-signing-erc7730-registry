      "calc_token_amount(uint256[2],bool)": {
        "intent": "Calculate token amount for deposit or withdrawal",
        "fields": [
          {
            "label": "Amounts",
            "format": "amount",
            "params": null,
            "path": "#._amounts.[]",
            "isRequired": true,
            "isIncluded": true
          },
          {
            "label": "Is Deposit",
            "format": "raw",
            "params": null,
            "path": "#._is_deposit",
            "isRequired": true,
            "isIncluded": true
          }
        ],
        "excluded": null,
        "required": [
          "#._amounts.[]",
          "#._is_deposit"
        ]
      },
      "add_liquidity(uint256[2],uint256)": {
        "intent": "Add liquidity to the pool",
        "fields": [
          {
            "label": "Amounts to Deposit",
            "format": "tokenAmount",
            "params": {
              "tokenPath": "#.coins"
            },
            "path": "#._amounts.[]",
            "isRequired": true,
            "isIncluded": true
          },
          {
            "label": "Minimum Mint Amount",
            "format": "tokenAmount",
            "params": {
              "tokenPath": "#.lp_token"
            },
            "path": "#._min_mint_amount",
            "isRequired": true,
            "isIncluded": true
          }
        ],
        "excluded": null,
        "required": [
          "#._amounts.[]",
          "#._min_mint_amount"
        ]
      },
      "get_dy(int128,int128,uint256)": {
        "intent": "Exchange tokens between coins",
        "fields": [
          {
            "label": "Source Coin Index",
            "format": "raw",
            "params": null,
            "path": "#.i",
            "isRequired": true,
            "isIncluded": true
          },
          {
            "label": "Destination Coin Index",
            "format": "raw",
            "params": null,
            "path": "#.j",
            "isRequired": true,
            "isIncluded": true
          },
          {
            "label": "Amount to Exchange",
            "format": "tokenAmount",
            "params": {
              "tokenPath": "#.coins[i]"
            },
            "path": "#._dx",
            "isRequired": true,
            "isIncluded": true
          }
        ],
        "excluded": null,
        "required": [
          "#.i",
          "#.j",
          "#._dx"
        ]
      },
      "exchange(int128,int128,uint256,uint256)": {
        "intent": "Exchange one token for another",
        "fields": [
          {
            "label": "Source Token Index (i)",
            "format": "raw",
            "params": null,
            "path": "#.i",
            "isRequired": true,
            "isIncluded": true
          },
          {
            "label": "Destination Token Index (j)",
            "format": "raw",
            "params": null,
            "path": "#.j",
            "isRequired": true,
            "isIncluded": true
          },
          {
            "label": "Amount to Exchange (Dx)",
            "format": "tokenAmount",
            "params": {
              "tokenPath": "#.coins[i]"
            },
            "path": "#._dx",
            "isRequired": true,
            "isIncluded": true
          },
