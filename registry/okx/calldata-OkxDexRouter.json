              { "label": "Amount to Send", "format": "tokenAmount", "path": "fromTokenAmount", "params": { "tokenPath": "fromToken" } },
              {
                "label": "Minimum amount to Receive",
                "format": "tokenAmount",
                "path": "minReturnAmount",
                "params": { "tokenPath": "toToken" }
              },
              { "label": "Deadline", "format": "date", "path": "deadLine", "params": { "encoding": "timestamp" } }
            ]
          },
          { "label": "Batch Amounts", "format": "amount", "path": "#.batchesAmount.[]" },
          {
            "path": "#.batches.[].[]",
            "fields": [
              {
                "label": "Adapter addresses",
                "format": "addressName",
                "params": { "types": ["wallet", "eoa", "contract", "token", "collection"] },
                "path": "mixAdapters.[]"
              },
              { "label": "Assets to", "format": "addressName", "params": { "types": ["token"] }, "path": "assetTo.[]" },
              { "label": "Raw Data", "format": "raw", "path": "rawData.[]" },
              { "label": "Raw Extra Data", "format": "raw", "path": "extraData.[]" },
              { "label": "Assets from", "format": "raw", "path": "fromToken" }
            ]
          }
        ],
        "required": ["#.baseRequest.fromToken", "#.baseRequest.toToken", "#.baseRequest.fromTokenAmount", "#.baseRequest.minReturnAmount"],
        "excluded": ["#.requests.[]"]
      }
    }
  }
}
