{
    "$schema": "../../specs/erc7730-v1.schema.json",
    "context": {
      "$id": "kRBTC",
      "contract": {
        "deployments": [
          {
            "chainId": 1,
            "address": "0xdeadbeef00000000000000000000000000000000"
          }
        ],
        "abi": "https://3142913783-files.gitbook.io/~/files/v0/b/gitbook-x-prod.appspot.com/o/spaces%2FVbInyuchDHnYPC3cAne6%2Fuploads%2F3R7Za7C083XV1eKOTENg%2FCRBTC.json?alt=media&token=254a9ca1-284f-43db-9d10-bea6802b78c0"
      }
    },
    "metadata": {
      "owner": "Tropykus Protocol",
      "info": {
        "legalName": "Tropykus Group LTD",
        "url": "https://tropykus.com/"
      }
    },
    "display": {
      "formats": {
        "mint": {
          "$id": "deposit",
          "intent": "Deposit",
          "fields": [
            {
              "label": "Deposit Amount",
              "format": "amount",
              "path": "@.value"
            }
          ],
          "required": [
            "@.value"
          ]
        },
        "borrow(uint256)": {
          "$id": "borrow",
          "intent": "Borrow",
          "fields": [
            {
              "label": "Borrow Amount",
              "format": "raw",
              "path": "#.borrowAmount"
            }
          ],
          "required": [
            "#.borrowAmount"
          ]
        },
        "redeem(uint256)": {
          "$id": "withdraw",
          "intent": "Withdraw",
          "fields": [
            {
              "label": "Withdraw Amount",
              "format": "raw",
              "path": "#.redeemTokens"
            }
          ],
          "required": [
            "#.redeemTokens"
          ]
        },
        "repayBorrow": {
          "$id": "repay",
          "intent": "Repay",
          "fields": [
            {
              "label": "Repay Amount",
              "format": "amount",
              "path": "@.value"
            }
          ],
          "required": [
            "@.value"
          ]
        }
      }
    }
  }