        "intent": "Moves `amount` tokens from the caller's account to `to`",
        "fields": [
          { "label": "To", "format": "addressName", "params": { "types": ["eoa", "wallet"] }, "path": "#.to" },
          { "label": "Amount", "format": "tokenAmount", "path": "amount", "params": { "token": "$.metadata.constants.swellToken" } }
        ]
      },
      "transferFrom(address,address,uint256)": {
        "intent": "Moves `amount` tokens from `from` to `to` using the allowance mechanism",
        "fields": [
          { "label": "From", "format": "addressName", "params": { "types": ["eoa", "wallet"] }, "path": "#.from" },
          { "label": "To", "format": "addressName", "params": { "types": ["eoa", "wallet"] }, "path": "#.to" },
          { "label": "Amount", "format": "tokenAmount", "path": "amount", "params": { "token": "$.metadata.constants.swellToken" } }
        ]
      },
