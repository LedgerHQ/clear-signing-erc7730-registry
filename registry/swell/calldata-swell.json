          { "label": "Owner", "format": "addressName", "params": { "types": ["eoa", "wallet"] }, "path": "#.owner" },
          { "label": "Spender", "format": "addressName", "params": { "types": ["contract"] }, "path": "#.spender" }
        ]
      },
