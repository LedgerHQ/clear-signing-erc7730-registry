        ]
      },
      "setNodeOperatorRewardPercentage(uint256)": {
        "intent": "Set node operator reward percentage",
        "fields": [{ "label": "New Node Operator Reward Percentage", "format": "raw", "path": "#._newNodeOperatorRewardPercentage" }]
