        "fields": [
          { "label": "Pre Reward ETH Reserves", "format": "raw", "path": "#._preRewardETHReserves" },
          { "label": "New ETH Rewards", "format": "raw", "path": "#._newETHRewards" },
          { "label": "Rsw ETH Total Supply", "format": "raw", "path": "#._rswETHTotalSupply" }
